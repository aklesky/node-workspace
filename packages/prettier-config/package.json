{
    "name": "@aklesky/prettier-config",
    "version": "1.0.0",
    "description": "Shareable prettier configuration.",
    "author": "Aleksei Semiglasov <aleksey.semiglasov@gmail.com>",
    "homepage": "https://github.com/aklesky/node-workspace#readme",
    "license": "MIT",
    "type": "module",
    "main": "dist/index.js",
    "prettier": "./dist/index.js",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.js"
        },
        "./config.js": {
            "require": "./dist/config.js",
            "import": "./dist/config.js"
        },
        "./package.json": "./package.json"
    },
    "publishConfig": {
        "access": "public"
    },
    "directories": {
        "dist": "dist"
    },
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aklesky/node-workspace.git"
    },
    "scripts": {
        "clean": "rimraf -rf ./dist",
        "build": "tsc --project ./tsconfig.build.json"
    },
    "bugs": {
        "url": "https://github.com/aklesky/node-workspace/issues"
    },
    "devDependencies": {
        "@aklesky/ts-config": "workspace:*"
    },
    "dependencies": {
        "prettier": "3.4.2"
    }
}
