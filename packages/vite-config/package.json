{
    "name": "@aklesky/vite-config",
    "version": "1.0.0",
    "description": "List of configurations for vite setup",
    "author": "Aleksei Semiglasov <aleksey.semiglasov@gmail.com>",
    "homepage": "https://github.com/aklesky/node-workspace#readme",
    "license": "MIT",
    "type": "module",
    "module": "dist/index.js",
    "exports": {
        "./*": "./dist/*"
    },
    "publishConfig": {
        "access": "public"
    },
    "directories": {
        "dist": "dist"
    },
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aklesky/node-workspace.git"
    },
    "scripts": {
        "clean": "rimraf -rf ./dist",
        "build": "tsc --project ./tsconfig.build.json"
    },
    "bugs": {
        "url": "https://github.com/aklesky/node-workspace/issues"
    },
    "dependencies": {
        "@vitejs/plugin-legacy": "6.0.0",
        "sass": "1.83.0",
        "terser": "5.37.0",
        "vite": "6.0.3"
    },
    "devDependencies": {
        "@aklesky/ts-config": "workspace:*"
    }
}
