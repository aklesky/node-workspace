{
    "name": "@aklesky/eslint-config",
    "version": "1.0.0",
    "description": "Shareable collection of eslint configurations.",
    "author": "Aleksei Semiglasov <aleksey.semiglasov@gmail.com>",
    "homepage": "https://github.com/aklesky/node-workspace#readme",
    "license": "MIT",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        "./*": "./dist/*"
    },
    "directories": {
        "dist": "dist"
    },
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aklesky/node-workspace.git"
    },
    "scripts": {
        "clean": "rimraf -rf ./dist",
        "build": "tsc --project ./tsconfig.build.json"
    },
    "bugs": {
        "url": "https://github.com/aklesky/node-workspace/issues"
    },
    "devDependencies": {
        "@aklesky/ts-config": "workspace:*"
    },
    "dependencies": {
        "@eslint/js": "9.16.0",
        "@types/eslint": "9.6.1",
        "@typescript-eslint/utils": "8.18.0",
        "typescript-eslint": "8.18.0",
        "eslint": "9.16.0",
        "globals": "15.13.0"
    }
}
