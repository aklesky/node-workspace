        "url": "https://github.com/aklesky/node-workspace/issues"
    },
    "dependencies": {
        "autoprefixer": "10.4.20",
        "cssnano": "7.0.6",
        "postcss": "8.4.49",
        "postcss-load-config": "6.0.1",
        "postcss-nesting": "13.0.1"
    },
    "devDependencies": {
        "@aklesky/ts-config": "workspace:*"
