{
    "name": "@aklesky/tailwind-config",
    "version": "1.0.0",
    "description": "Tailwind Css configuration presets",
    "author": "Aleksei Semiglasov <aleksey.semiglasov@gmail.com>",
    "homepage": "https://github.com/aklesky/node-workspace#readme",
    "license": "MIT",
    "type": "module",
    "exports": {
        "./*": "./dist/*"
    },
    "publishConfig": {
        "access": "public"
    },
    "directories": {
        "dist": "dist"
    },
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aklesky/node-workspace.git"
    },
    "scripts": {
        "clean": "rimraf -rf ./dist",
        "build": "tsc --project ./tsconfig.build.json"
    },
    "bugs": {
        "url": "https://github.com/aklesky/node-workspace/issues"
    },
    "dependencies": {
        "@tailwindcss/aspect-ratio": "0.4.2",
        "@tailwindcss/forms": "0.5.9",
        "@tailwindcss/line-clamp": "0.4.4",
        "@tailwindcss/typography": "0.5.15",
        "tailwindcss": "3.4.16"
    },
    "devDependencies": {
        "@aklesky/ts-config": "workspace:*"
    }
}
